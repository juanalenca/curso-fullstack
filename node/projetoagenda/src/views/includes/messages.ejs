<% if(errors.lenght > 0) { %>
<div class="row">
    <div class="col my-3">
      <div class="alert-danger">
        <% errors.forEach(erro => { %>
          <%= erro %><br>
        <% }) %>
      </div>
    </div>
</div>
<% } %>

<% if(sucess.lenght > 0) { %>
  <div class="row">
      <div class="col my-3">
        <div class="alert-sucess">
          <% sucess.forEach(sucess => { %>
            <%= sucess %><br>
          <% }) %>
        </div>
      </div>
  </div>
  <% } %>